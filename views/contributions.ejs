<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">

  <title>My Contributions</title>

  <link rel="stylesheet" href="/styles/contributions.css" type="text/css" />
  <script type="text/javascript" src="/routes/contributions.js"></script>
</head>

<body>
  <nav>
    <!-- <div class="nav-left"> -->
    <div class="nav-home">
      <h3>Home</h3>
    </div>
    <div class="nav-favourites">
      <h3>Favourites</h3>
    </div>
    <!-- </div> -->
    <div class="nav-logout">Logout</div>
  </nav>
  <main>
    <h1>My Contributions</h1>
    <h5>Total Number of Contributions: <%= contributions.length %>
    </h5>
    <section id="contribution-list">
      <% for(let contrib of contributions) { %>
        <ol>
          <li>
            <%= contrib.map_id %>,
              <%= contrib.title %>,
          </li>
        </ol>
        <% } %>
    </section>
  </main>
</body>

</html>
